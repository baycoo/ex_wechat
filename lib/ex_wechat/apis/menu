#--------------------
#  menu
#--------------------
# get the menu
function: get_menu
path: /menu/get
http: get
params: access_token

# create the menu
function: create_menu
path: /menu/create
http: post
params: access_token

# delete the menu
function: delete_menu
path: /menu/delete
http: get
params: access_token

# special menu create
function: create_special_menu
path: /menu/addconditional
http: post
params: access_token

# delete special menu
function: delete_special_menu
path: /menu/delconditional
http: post
params: access_token

# get all the menu config
function: get_menu_conf
path: /get_current_selfmenu_info
http: get
params: access_token
